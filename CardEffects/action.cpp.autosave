#include "action.h"

// -------------------
// CONSTRUCTORS
// -------------------

// Default constructor
Action::Action()
{
}

// Constructor when given a string as the command
Action::Action(QString command) {
    stringCommand = command;
}

Action::~Action() {}

// -------------------
// METHODS
// -------------------

// returns  the original string command
QString Action::GetStringCommand() {
    return stringCommand;
}

